<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-03-17T10:56:25Z</date>
    <groups>
        <group>
            <name>Templates/Applications/Raycare</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Raycare Health Check</template>
            <name>Raycare Health Check</name>
            <groups>
                <group>
                    <name>Templates/Applications/Raycare</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Raycare</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Raycare Aggregated Health Check</name>
                    <type>HTTP_AGENT</type>
                    <key>raycare.report.aggregated</key>
                    <delay>5m</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Raycare</name>
                        </application>
                    </applications>
                    <timeout>20s</timeout>
                    <url>{$AGGREGATEDREPORTURL}</url>
                </item>
                <item>
                    <name>Raycare Aggregated Report</name>
                    <type>DEPENDENT</type>
                    <key>raycare.report.aggregatedreport</key>
                    <delay>0</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Raycare</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.aggregatedReport</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>raycare.report.aggregated</key>
                    </master_item>
                </item>
                <item>
                    <name>Raycare Aggregated Health Status</name>
                    <type>DEPENDENT</type>
                    <key>raycare.report.aggregatedstatus</key>
                    <delay>0</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Raycare</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.aggregatedStatus</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>raycare.report.aggregated</key>
                    </master_item>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Raycare Aggregated Health Discovery</name>
                    <type>DEPENDENT</type>
                    <key>raycare.report.discovery</key>
                    <delay>0</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Raycare Outbox Status On {#NODENAME}</name>
                            <type>DEPENDENT</type>
                            <key>raycare.outbox.status.[{#NODENAME}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Raycare</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..reports[2].status</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>raycare.report.[{#NODENAME}]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;&quot;[\&quot;ok\&quot;]&quot;</expression>
                                    <name>Raycare Outbox Status Not OK on {#NODENAME}</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Raycare RabbitMQ Status On {#NODENAME}</name>
                            <type>DEPENDENT</type>
                            <key>raycare.rabbit.status.[{#NODENAME}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Raycare</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..reports[0].status</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>raycare.report.[{#NODENAME}]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;&quot;[\&quot;ok\&quot;]&quot;</expression>
                                    <name>Raycare RabbitMQ Status Not OK on {#NODENAME}</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Raycare report on {#NODENAME}</name>
                            <type>DEPENDENT</type>
                            <key>raycare.report.[{#NODENAME}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Raycare</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..[?(@.source ==&quot;{#NODENAME}&quot;)]</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>raycare.report.aggregated</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Raycare SqlHealth Status On {#NODENAME}</name>
                            <type>DEPENDENT</type>
                            <key>raycare.sql.status.[{#NODENAME}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Raycare</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..reports[1].status</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>raycare.report.[{#NODENAME}]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;&quot;[\&quot;ok\&quot;]&quot;</expression>
                                    <name>Raycare SqlHealth Status Not OK on {#NODENAME}</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Raycare status on {#NODENAME}</name>
                            <type>DEPENDENT</type>
                            <key>raycare.status.[{#NODENAME}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>Raycare</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$[0].status</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>raycare.report.[{#NODENAME}]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;&quot;ok&quot;</expression>
                                    <name>Raycare status not ok on {#NODENAME}</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>raycare.report.aggregated</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#NODENAME}</lld_macro>
                            <path>$.source</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.aggregatedReport.*</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$AGGREGATEDREPORTURL}</macro>
                    <value></value>
                    <description>The identityserver aggregated health URL</description>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
